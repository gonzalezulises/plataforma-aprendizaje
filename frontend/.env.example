# Frontend Environment Variables
# Copy this file to .env and fill in the values

# API URL (for production, set to your backend URL)
# In development, the Vite proxy handles /api requests
VITE_API_URL=

# WebSocket URL (for production)
VITE_WS_URL=

# Supabase Configuration (shared with rizo-web)
# Get these from your Supabase project settings (use anon key, not service key)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=eyJ...your-anon-key
